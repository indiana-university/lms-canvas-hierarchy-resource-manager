{
  "title": "Canvas Node Manager",
  "description": "Tool used to maintain templates at IU.",
  "oidc_initiation_url": "http://localhost:8080/lti/login_initiation/lms_lti_hrm_manager",
  "target_link_uri": "http://localhost:8080/app/manager",
  "extensions": [
    {
      "domain": "localhost",
      "platform": "canvas.instructure.com",
      "privacy_level": "public",
      "settings": {
        "placements": [
          {
            "enabled": true,
            "default": "disabled",
            "placement": "account_navigation",
            "message_type": "LtiResourceLinkRequest"
          }
        ]
      }
    }
  ],
  "public_jwk_url": "http://localhost:8080/.well-known/jwks.json",
  "custom_fields": {
    "instructure_membership_roles": "$com.Instructure.membership.roles",
    "canvas_course_id": "$Canvas.course.id",
    "canvas_user_login_id": "$Canvas.user.loginId"
  }
}